# top-most EditorConfig file
root = true

# Global settings
[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.java]
max_line_length = 120

[*.properties]
charset = latin1
